{
    "Name": "FunSDK-small",
    "URL": "fungible://FunSDK-small",
    "BuildVerbosityFormat": "dV",
    "BuildCommands":
    {
        "Build": "./scripts/bob --sdkup -v current --preserve-mtime -C %CCHROOT%",
        "Clean": "./scripts/bob --clean-sdk"
    },
    "BuildStyles":
    {
        "azure": "--azure",
        "azure-az": "--azure-az"
    },
    "TagPrefix": "bld_",
    "Exports":
    {
        "SDKDIR": "%WORKSPACE%/FunSDK-small",
        "SDK_INSTALL_DIR": "%WORKSPACE%/FunSDK-small"
    },
    "MinimumBuildCycle": "Iterative"
}