{
    "Name": "FunSDK-small",
    "URL": "fungible://FunSDK-small",
    "BuildVerbosityFormat": "dV",
    "BuildCommand": "./scripts/bob --sdkup -v current",
    "CleanCommand": "./scripts/bob --clean-all",
    "BuildStyles":
    {
        "azure": "--azure",
        "azure-az": "--azure-az"
    },
    "TagPrefix": "bld_",
    "Exports":
    {
        "SDKDIR": "%WORKSPACE%/FunSDK-small",
        "SDK_INSTALL_DIR": "%WORKSPACE%/FunSDK-small"
    },
    "MinimumBuildCycle": "Iterative"
}