{
    "Name": "FunOS",
    "URL": "fungible://FunOS",
    "BuildTool": "make",
    "BuildVerbosityFormat": "2Vl=1",
    "BuildQuietFormat": "1dq",
    "BuildCommands":
    {
        "Build": "make -j%NCPU% MACHINE=%TARGET% SIGN=1",
        "Clean": "make clean"
    },
    "BuildStyles":
    {
        "issu": "ISSU_XDATA=1"
    },
    "BuildDirectory": "./build",
    "TagPrefix": "bld_",
    "Images":
    {
        "default": "stripped",
        "rich": "funos-%TARGET%",
        "stripped": "funos-%TARGET%.stripped",
        "elf": "funos-%TARGET%.stripped",
        "signed": "funos-%TARGET%.signed"
    },
    "Dependencies":
    [
        "FunSDK"
    ]
}