{
    "Name": "FunSDK",
    "URL": "fungible://FunSDK-small",
    "BuildVerbosityFormat": "dv",
    "BuildCommands":
    {
        "InstallSDK": "./scripts/bob --sdkup -v %SDK_VERSION% --preserve-mtime -C %CCHROOT%",
        "CleanSDK": "./scripts/bob --clean-sdk",
        "Build": "git archive -o ./build/%INSTANCE%/FunSDK.src.tgz --format tgz --prefix FunSDK/ HEAD",
        "Clean": "rm -f ./build/%INSTANCE%/FunSDK.src.tgz"
    },
    "BuildTool": "git archive",
    "BuildDirectory": "./build/%INSTANCE%",
    "TagPrefix": "bld_",
    "Exports":
    {
        "SDKDIR": "%WORKSPACE%/FunSDK",
        "SDK_INSTALL_DIR": "%WORKSPACE%/FunSDK"
    },
    "Images":
    {
        "default": "gzip",
        "gzip": "FunSDK.src.tgz"
    }
}