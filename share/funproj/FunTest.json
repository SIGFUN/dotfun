{
    "Name": "FunTest",
    "URL": "fungible://FunTest",
    "BuildVerbosityFormat": "dv",
    "BuildTool": "git archive",
    "BuildCommands":
    {
        "Build": "git archive -o ./FunTest.%INSTANCE%.tar.gz --format tgz HEAD",
        "Clean": "rm -f ./FunTest.%INSTANCE%.tar.gz"
    },
    "BuildDirectory": ".",
    "TagPrefix": "bld_",
    "Images":
    {
        "default": "gzip",
        "gzip": "FunTest.%INSTANCE%.tar.gz"
    },
    "MinimumBuildCycle": "Full"
}